[user]
        name = Jorin Slaybaugh
        email = jslaybaugh@gmail.com
[credential]
        helper = osxkeychain
[color]
        ui = true
        branch = auto
        status = auto
        diff = auto
[push]
        default = matching
[merge]
        keepBackup = false
        tool = custom

[mergetool "custom"]
        cmd = /Applications/p4merge.app/Contents/Resources/launchp4merge "$PWD/$BASE" "$PWD/$REMOTE" "$PWD/$LOCAL" "$PWD/$MERGED"
        keepTemporaries = false
        trustExitCode = false
        keepBackup = false
[core]
        excludesfile = /Users/jslaybaugh/.gitignore_global
[mergetool]
        keepBackup = false
